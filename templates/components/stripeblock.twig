<div class="stripe-block {{style}} {{class}}">
    <div class="stripe-block-content-wrapper {{wrapperclass}}">
        {% block stripecontent %}
            {# Conten here #}
        {% endblock %}
    </div>
    {% if function ('is_front_page') %}
        <div class="stripe-block-stripe-container">
            <div class="stripe-block-stripe stripe-block-stripe-1"></div>
            <div class="stripe-block-stripe stripe-block-stripe-2"></div>
        </div>
    {% else %}
        <div class="stripe-block-stripe-container"></div>
    {% endif %}
</div>